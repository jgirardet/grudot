{
  "name": "godot4-rust",
  "displayName": "Godot4 Rust",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "godot4-rust.insertOnReady",
        "title": "Godot4-Rust: Insert OnReady field"
      },
      {
        "command": "godot4-rust.setGodotProject",
        "title": "Godot4-Rust: Set Godot Project"
      },
      {
        "command": "godot4-rust.newGodotClass",
        "title": "Godot4-Rust: Create a new GodotClass from Godot Scene"
      }
    ],
    "configuration": {
      "title": "Godot4 Rust",
      "properties": {
        "godot4-rust.godotProjectFilePath": {
          "description": "The path to .godot project file",
          "type": "string",
          "default": null
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "lint": "eslint src"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "eslint": "^9.23.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "glob": "^11.0.1",
    "string-ts": "^2.2.1",
    "ts-case-convert": "^2.1.0",
    "tscn2json": "^1.0.0",
    "winston": "^3.17.0",
    "winston-transport-vscode": "^0.1.0"
  }
}
